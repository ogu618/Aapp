<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="99dp"
        android:layout_height="65dp"
        android:text="0"
        app:layout_constraintEnd_toEndOf="@+id/button8"
        tools:layout_editor_absoluteY="115dp" />


    <Button
        android:id="@+id/button5"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="7"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="274dp" />
    <Button
        android:id="@+id/button6"
        android:layout_width="70dp"
        android:layout_height="49dp"
        android:layout_margin="20dp"
        android:text="8"

        app:layout_constraintStart_toEndOf="@id/button5"
        tools:layout_editor_absoluteY="274dp"

        />

    <Button
        android:id="@+id/button7"
        android:layout_width="70dp"
        android:layout_height="49dp"
        android:layout_margin="20dp"
        android:layout_marginStart="20dp"

        android:text="9"


        app:layout_constraintStart_toEndOf="@id/button6"
        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="รท"
        android:layout_margin="20dp"
        app:layout_constraintStart_toEndOf="@id/button7"



        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_d4"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="4"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button5"
        tools:layout_editor_absoluteY="274dp" />
    <Button
        android:id="@+id/button_d5"
        android:layout_width="70dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"

        android:text="5"

        app:layout_constraintStart_toEndOf="@id/button_d4"
        app:layout_constraintTop_toTopOf="@id/button_d4"
        tools:layout_editor_absoluteY="274dp"

        />
    <Button
        android:id="@+id/button_d6"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="6"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@id/button_d5"
        app:layout_constraintTop_toTopOf="@id/button_d4"

        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_dtime"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="รท"

        app:layout_constraintStart_toEndOf="@id/button_d6"

        app:layout_constraintTop_toTopOf="@id/button_d4"
        android:layout_marginStart="20dp"

        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_d1"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="1"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_d4"
        tools:layout_editor_absoluteY="274dp" />
    <Button
        android:id="@+id/button_d2"
        android:layout_width="70dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"

        android:text="2"

        app:layout_constraintStart_toEndOf="@id/button_d1"
        app:layout_constraintTop_toTopOf="@id/button_d1"
        tools:layout_editor_absoluteY="274dp"

        />
    <Button
        android:id="@+id/button_d3"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="3"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@id/button_d2"
        app:layout_constraintTop_toTopOf="@id/button_d1"

        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_dmi"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="-"

        app:layout_constraintStart_toEndOf="@id/button_d3"

        app:layout_constraintTop_toTopOf="@id/button_d1"
        android:layout_marginStart="20dp"

        tools:layout_editor_absoluteY="274dp" />


    <Button
        android:id="@+id/button_dAC"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="AC"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_d1"
        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_d0"
        android:layout_width="70dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"

        android:text="0"

        app:layout_constraintStart_toEndOf="@id/button_dAC"
        app:layout_constraintTop_toTopOf="@id/button_dAC"
        tools:layout_editor_absoluteY="274dp"

        />
    <Button
        android:id="@+id/button_deq"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="="
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@id/button_d0"
        app:layout_constraintTop_toTopOf="@id/button_dAC"

        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/button_dpu"
        android:layout_width="70dp"
        android:layout_height="49dp"

        android:text="+"

        app:layout_constraintStart_toEndOf="@id/button_deq"

        app:layout_constraintTop_toTopOf="@id/button_dAC"
        android:layout_marginStart="20dp"

        tools:layout_editor_absoluteY="274dp" />

</androidx.constraintlayout.widget.ConstraintLayout>